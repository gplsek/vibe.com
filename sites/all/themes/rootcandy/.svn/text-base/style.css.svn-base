}

#content-wrap #inside {
  margin: 0 10px 0 0;
  padding: 0;
}

#content {
  padding-left: 10px;
  padding-top: 0px;
  background: #fff;
}

.sidebar-left #content {
  padding-left: 210px;
}

.sidebar-right #content {
  padding-right: 210px;
}

.sidebars #content {
  padding-left: 210px;
  padding-right: 210px;
}

#sidebar-right {
  width: 210px;
