}

div.admin-content {
  padding: 10px;
  min-height: 200px;
}

#placeholder {
  height: 1em;
}

#content-wrap {
  background: white;
  min-width: 780px;
  max-width: 1260px;
  margin: 0 auto 10px auto;
  clear: both;
